{
  "name": "sumocoders/framework-user-bundle",
  "type": "symfony-bundle",
  "description": "The user bundle for our mini-framework",
  "keywords": ["Framework", "SumoCoders"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tijs Verkoyen",
      "email": "tijs@sumocoders.be"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type":"vcs",
      "url":"git@git.sumocoders.be:tijs/frameworkerrorbundle.git"
    },
    {
      "type":"vcs",
      "url":"git@git.sumocoders.be:tijs/frameworkcorebundle.git"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "friendsofsymfony/user-bundle": "~2.0@dev",
    "knplabs/knp-paginator-bundle": "~2.4",
    "sumocoders/framework-core-bundle": "dev-master"
  },
  "autoload": {
    "psr-0": { "SumoCoders\\FrameworkUserBundle": "" }
  },
  "target-dir": "SumoCoders/FrameworkUserBundle"
}
